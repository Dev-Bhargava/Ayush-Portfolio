<div data-section-id="{{ section.id }}" data-section-type="section-banner-v10" style="{% if section.settings.margin_top != blank %}margin-top: {{section.settings.margin_top}}px;{%endif%} {% if section.settings.margin_bottom != blank %}margin-bottom: {{section.settings.margin_bottom}}px;{% endif %}">
  <div class="section-banner-v10" style="background-image: url({% if section.settings.img_banner != blank %}{{section.settings.img_banner | img_url: 'master'}} {% else %} //via.placeholder.com/1920x900{% endif %})">
    <div class="container container-v2">
      <div class="row ">  
        <div class="col-12">
          <div class="content-heading text-center">
            {% if section.settings.sub_heading != blank %}
            <h6 class="sub-heading" style="color: {{section.settings.color_sub_heading}}">{{section.settings.sub_heading}}</h6>
            {% endif %}
            {% if section.settings.title_heading != blank %}
            <h4 class="heading" style="color: {{section.settings.color_heading}}">{{section.settings.title_heading}}</h4>
            {% endif %}
          </div>
        </div>
        <div class="col-md-4 col-lg-4" >
          <div class="item-banner ">
            <div class="content-banner">
              {% if section.settings.title_banner != blank %}
              <h3 class="title-banner" style="color: {{section.settings.color_title}}">{{section.settings.title_banner}}</h3>
              {% endif %}
              {% if section.settings.price_dola != blank %}
              <p class="price-dola "style="color: {{section.settings.color_price_dola}}">{{section.settings.price_dola}}</p>
              {% endif %}
              {% if section.settings.elementor_typo != blank %}
              <p class="elementor-typo ">{{section.settings.elementor_typo}}</p>
              {% endif %}
            </div>
            <div class="elementor-price" style="background: {{section.settings.bg_price}}">
              {% if section.settings.price != blank %}
              <span class="price" style="color: {{section.settings.color_price}}">{{section.settings.price}}</span>
              {% endif %}
              {% if section.settings.price_description != blank %}
              <span class="price-description" style="color: {{section.settings.color_price_description}}">{{section.settings.price_description}}</span>
              {% endif %}
            </div>
            {% if section.settings.features_title != blank %}
            <h4 class="features-title" style="color: {{section.settings.color_features_title}}">{{section.settings.features_title}}</h4>
            {% endif %}
            <div class="menu">
              <ul class="cat-menu2">
                {% if section.blocks.size > 0 %}
                {% for block in section.blocks %}
                {% if block.type == 'banner' %}
                <li>
                  {% if block.settings.cart_menu != blank %}
                  <span style="color: {{block.settings.color_cart_menu}}">{{block.settings.cart_menu}}
                  </span>
                  {% endif %}
                </li>
                {% endif %}
                {% endfor %}
                {% endif %}
              </ul>
            </div>
            <div class="button-banner">
              {% if section.settings.button_banner != blank %}
              <a href="{{section.settings.url_banner}}" class="button-shop button-shop-{{ section.id }}" title="">{{section.settings.button_banner}}</a>
              {% endif %}
            </div>
          </div>
        </div>
        <div class="col-md-4 col-lg-4">
          <div class="item-banner">
            <div class="content-banner">
              {% if section.settings.title_banner2 != blank %}
              <h3 class="title-banner" style="color: {{section.settings.color_title}}">{{section.settings.title_banner2}}</h3>
